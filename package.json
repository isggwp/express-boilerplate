{
  "name": "remotesaga-js",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "npx tsc -w",
    "dev": "npx nodemon",
    "lint": "npx eslint ./src ",
    "lint:fix": "npx eslint ./src --fix",
    "build": "npx tsc && cp .env ./build/.env",
    "prod": "node build index.js",
    "prepare": "npx husky install",
    "check-types": "npx tsc --noEmit --pretty",
    "check-lint": "npx eslint ./src",
    "check-format": "npx prettier . --check"
  },
  "husky": {
    "hooks": {
      "pre-commmit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "remote jobs list",
  "repository": "https://github.com/isggwp/express-boilerplate.git",
  "author": "indra susila"
}
